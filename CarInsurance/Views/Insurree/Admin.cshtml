@model IEnumerable<CarInsurance.Models.Insurance>
@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Quote</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td> @item.FirstName </td>
            <td> @item.LastName </td>
            <td> @item.EmailAddress </td>
            <td> @item.Quote </td>
        </tr>
    }
</table>